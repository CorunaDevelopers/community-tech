{
  "title": "Manual event JSON schema",
  "type": "object",
  "description": "",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "additionalProperties": false,
  "required": ["title", "date", "url"],
  "properties": {
    "title": {
      "type": "string",
      "description": "Eventname"
